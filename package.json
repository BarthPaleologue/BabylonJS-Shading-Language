{
    "name": "babylonjs-shading-language",
    "version": "1.2.0",
    "description": "A thin wrapper around BabylonJS NodeMaterial to make it easier to read and write for those who don't want to use the online editor",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BarthPaleologue/BabylonJS-Shading-Language.git"
    },
    "scripts": {
        "fmt": "prettier --write .",
        "lint": "eslint .",
        "build": "tsc",
        "docs": "typedoc --out docs src/index.ts"
    },
    "author": "Barthélemy Paléologue",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "prettier": "^3.5.2",
        "typedoc": "^0.27.9",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.1"
    },
    "dependencies": {
        "@babylonjs/core": "^7.50.0"
    }
}
